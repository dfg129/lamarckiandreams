		"clean": "rm -rf .svelte-kit .vercel_build_output build node_modules"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "1.0.0-next.36",
		"@sveltejs/kit": "next",
		"@tailwindcss/typography": "0.5.0",
		"autoprefixer": "10.4.2",
		"cssnano": "5.0.15",
		"daisyui": "1.22.2",
		"mdsvex": "0.9.8",
		"mdsvex-relative-images": "1.0.1",
		"postcss": "8.4.5",
		"postcss-load-config": "3.1.1",
		"prettier": "2.5.1",
		"prettier-plugin-svelte": "2.5.1",
		"rehype-autolink-headings": "6.1.1",
		"rehype-slug": "5.0.1",
		"remark-external-links": "9.0.1",
		"remark-reading-time": "1.0.1",
		"svelte": "3.46.1",
		"svelte-preprocess": "4.10.1",
		"tailwindcss": "3.0.13",
		"theme-change": "2.0.2",
		"truncate-html": "1.0.4"
